{
  "build": {
    "development": {
      "developmentClient": "true",
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "distribution": "store",
        "credentialsSource": "remote"
      }
    }
  },
  "cli": {
    "appVersionSource": "remote"
  }
}